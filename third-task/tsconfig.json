{
  "compilerOptions": {
    "target": "ESNext",
     "jsx": "react-jsx",
    "module": "CommonJS",
     "outDir": "../second-task/dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "shared/*": ["./src/shared/*"],
      "entities/*": ["./src/entities/*"],
      "features/*": ["./src/features/*"],
      "widgets/*": ["./src/widgets/*"],
    }
  },
}
